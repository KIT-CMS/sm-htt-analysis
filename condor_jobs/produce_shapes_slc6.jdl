universe = docker
docker_image = mschnepf/slc6-condocker
executable = batch_shapes.sh
output = out/$(Process).out
error = error/$(Process).err
log = log/$(Process).log
getenv = True
Requirements = (TARGET.CLOUDSITE == "ekpsupermachines") && ( (Target.ProvidesCPU == True) && (TARGET.ProvidesEKPResources == True ) )
+RequestWalltime = 36000
+ExperimentalJob = True
RequestMemory = 20000
request_disk = 204800
RequestCpus = 12
accounting_group = higgs
queue arguments from arguments.txt
